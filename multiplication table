#include<stdio.h>
#include<string.h>
int  Printf(int n);
//打印乘法口诀表
int main()
{
	int n=0;
	printf("please input the number of lines:\n");
	scanf_s("%d", n);
	Printf(n);
	return 0;
}
 int  Printf(int n)
{
	int i = 0, j = 0;
	for (i = 1; i <= n; i++)
	{
		for (j = 1; j <= i; j++)
		{
			printf("%d*%d=%d ", i, j, i*j);


		}
		printf("\n");
	}	
	return 1;
}



